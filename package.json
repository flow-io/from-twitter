{
  "name": "flow-from-twitter",
  "version": "0.0.0",
  "description": "Provides a stream interface for the Twitter stream API.",
  "author": {
    "name": "Athan Reines",
    "email": "kgryte@gmail.com"
  },
  "contributors": [
    {
      "name": "Athan Reines",
      "email": "kgryte@gmail.com"
    }
  ],
  "scripts": {
    "test": "./node_modules/.bin/mocha",
    "test-cov": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --dir ./reports/coverage -- -R spec",
    "coveralls": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --dir ./reports/coveralls/coverage --report lcovonly -- -R spec && cat ./reports/coveralls/coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./reports/coveralls"
  },
  "main": "./lib",
  "repository": {
    "type": "git",
    "url": "git://github.com/flow-io/from-twitter.git"
  },
  "keywords": [
    "stream",
    "flow",
    "flow.io",
    "twitter",
    "tweets",
    "readable",
    "readstream",
    "from",
    "source"
  ],
  "bugs": {
    "url": "https://github.com/flow-io/from-twitter/issues"
  },
  "dependencies": {
    "readable-stream": "^1.0.33",
    "validate.io-boolean-primitive": "^1.0.0",
    "validate.io-nonnegative": "^1.0.0",
    "validate.io-object": "^1.0.3",
    "validate.io-string-primitive": "^1.0.0"
  },
  "devDependencies": {
    "chai": "2.x.x",
    "mocha": "2.x.x",
    "coveralls": "^2.11.1",
    "istanbul": "^0.3.0",
    "flow-from-array": "^1.1.5",
    "flow-tostring": "^1.0.0",
    "flow-append": "^1.0.0",
    "flow-mock-read": "^1.0.0",
    "flow-mock-write": "^1.0.0"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://www.opensource.org/licenses/MIT"
    }
  ]
}
